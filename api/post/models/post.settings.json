{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Article": {
      "type": "richtext"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "excerpt": {
      "type": "string"
    },
    "views": {
      "type": "integer",
      "default": 0,
      "min": 0
    }
  }
}
